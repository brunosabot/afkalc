{
  "name": "afkalc",
  "version": "1.4.6",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "extract": "i18next --config i18next-parser.config.js",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:ts",
    "lint:js": "eslint --fix --ext .ts,.tsx ./",
    "lint:css": "stylelint --fix \"**/*.css\"",
    "lint:ts": "tsc -p tsconfig.json",
    "prettier": "prettier --write '**/*.@(ts|tsx|js|jsx|css)'",
    "test": "jest --watch",
    "test:lint-staged": "cross-env CI=true jest --findRelatedTests",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@mdi/js": "^6.4.95",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react-hooks": "^7.0.2",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "firebase": "^8.4.3",
    "i18next": "^21.4.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "i18next-http-middleware": "^3.1.4",
    "nanoid": "^3.1.30",
    "next": "12.0.3",
    "next-i18next": "^9.0.0",
    "next-images": "^1.8.2",
    "qrcode.react": "^1.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.14.2",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "globby": "^12.0.2",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "lint-staged": {
    "*.css": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "yarn run test:lint-staged",
      "eslint --ext .js,.jsx,.ts,.tsx",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
